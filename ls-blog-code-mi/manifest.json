{
    "manifest_version": 2,
    "name": "书路抢小米",
    "version": "1.0",
    "description": "抢小米9手机",
    "browser_action": {
	
	"default_icon": "icon.png"
    },
	"icons": {
 	   "16": "icon16.png",
 	   "48": "icon.png",
 	   "128": "icon128.png"
 	 },
    "content_scripts":[
        {
            "matches": ["http://*/*","https://*/*","file:///*/*"],
            "js": ["jquery.js","back.js"],
            "run_at":"document_end"
        }
    ],
    "permissions": [
        "tabs"
    ]
}
